{
  "plugins": [
    "@babel/plugin-syntax-class-properties"
  ],
  "presets": [
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "pragma": "Simurgh.createElement",
        "pragmaFrag": "DomFrag",
        "throwIfNamespace": false,
        "runtime": "classic"
      }
    ]
  ]
}