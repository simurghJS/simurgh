{
  "name": "simurgh",
  "app-name": "simurgh",
  "project-name": "simurgh",
  "author": "SimurghTech",
  "version": "0.1.2-beta.1",
  "icon": "./assets/icon/icon.png",
  "description": "js framework for client side web page rendering",
  "dependencies": {
    "extract-zip": "2.0.1",
    "simurgh-cli": "^1.0.4",
    "typescript": "4.1.3",
    "tslint": "6.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-typescript": "7.12.13",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.13",
    "extract-zip": "2.0.1"
  },
  "scripts": {
    "tsc":"tsc",
    "build": "npx babel *.* --out-dir dist/web --copy-files && npx babel  ./corejs ./dom --extensions '.ts,.js' --out-file dist/web/simurgh.js && npx babel ./src --out-dir dist/web/src --copy-files"
