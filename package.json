{
  "name": "simurgh",
  "app-name": "simurgh",
  "project-name": "simurgh",
  "author": "arnahit",
  "version": "0.6.3-beta.1",
  "description": "js library for ssr page rendering",
  "dependencies": {
    "jsdom": "^16.4.0",
    "simurgh-cli": "^1.1.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.1.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-typescript": "7.12.13",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.13",
    "@types/node": "^14.14.25",
    "extract-zip": "2.0.1"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "npm run build-root && npm run build-app && npm run build-bootstrap && npm run build-bin && npm run build-lib && npm run build-assets",
    "build-root": "npx babel *.* --out-dir dist/web --extensions '.ts,.tsx'  --copy-files",
    "build-app": "npx babel  ./core --extensions '.ts,.js' --out-dir dist/web/core",
    "build-bootstrap": "npx babel  ./bootstrap --extensions '.ts,.js' --out-dir dist/web/bootstrap",
    "build-bin": "npx babel  ./bin --extensions '.ts' --out-dir dist/web/bin   --copy-files",
    "build-lib": "npx babel  ./lib --extensions '.ts' --out-dir dist/web/lib   --copy-files",
    "build-assets": "npx babel  ./assets --extensions '.ts' --out-dir dist/web/assets   --copy-files"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simurghJS/simurgh.git"
  },
  "main": "index.js",
  "bin": {
    "simurgh": "./bin/index.js"
  }
}
